{
  "name": "prompt-firewall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "rm -rf node_modules/.prisma 2>/dev/null; prisma generate --schema=prisma/schema.prisma",
